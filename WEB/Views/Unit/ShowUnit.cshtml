@model Core.DTOs.UnitDto

<button class="btn btn-outline-secondary" onclick="window.history.go(-1);">
    <i class="bi bi-arrow-left"></i> Geri
</button>

<form asp-action="Delete" method="post" asp-route-id=@Model.Id asp-controller="Unit">
    <div class="form-group">
        <label asp-for="UnitName" class="control-label"></label>
        <input asp-for="UnitName" class="form-control"  readonly />
        <span asp-validation-for="UnitName" class="text-danger"></span>
    </div>
</form>


