@model Core.DTOs.ProjectDto

<button class="btn btn-outline-secondary" onclick="window.history.go(-1);">
    <i class="bi bi-arrow-left"></i> Geri
</button>

<form asp-action="Delete" method="post" asp-route-id =@Model.Id asp-controller="Project">
    <div class="form-group">
        <label asp-for="ProjectName" class="control-label"></label>
        <input asp-for="ProjectName" class="form-control"  readonly/>
        <span asp-validation-for="ProjectName" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="CustomerId"></label>
        <select asp-for="CustomerId" asp-items="ViewBag.customer"  disabled readonly class="form-control"></select>

    </div>

</form>
