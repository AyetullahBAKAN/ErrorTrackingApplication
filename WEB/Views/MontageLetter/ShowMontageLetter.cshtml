@model Core.DTOs.MontageLetterDto

<button class="btn btn-outline-secondary" onclick="window.history.go(-1);">
    <i class="bi bi-arrow-left"></i> Geri
</button>

<form asp-action="Delete" method="post" asp-route-id=@Model.Id asp-controller="MontageLetter">
    <div class="form-group">
        <label asp-for="MontageNumber" class="control-label"></label>
        <input asp-for="MontageNumber" class="form-control" readonly />
        <span asp-validation-for="MontageNumber" class="text-danger"></span>
    </div>

</form>
