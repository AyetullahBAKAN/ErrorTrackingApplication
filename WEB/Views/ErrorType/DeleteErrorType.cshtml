@model Core.DTOs.ErrorTypeDto

<button class="btn btn-outline-secondary" onclick="window.history.go(-1);">
    <i class="bi bi-arrow-left"></i> Geri
</button>

<form asp-action="Delete" method="post" asp-route-id=@Model.Id asp-controller="ErrorType">


    <div class="form-group">
        <label asp-for="UserId"></label>
        <select asp-for="UserId" asp-items="ViewBag.users" disabled class="form-control" readonly></select>

    </div>

    <div class="form-group">
        <label asp-for="ErrorMainTitleId"></label>
        <select asp-for="ErrorMainTitleId" asp-items="ViewBag.errorMainTitleList" disabled class="form-control" readonly></select>

    </div>

    <div class="form-group">
        <label asp-for="ErrorSubGroupId"></label>
        <select asp-for="ErrorSubGroupId" asp-items="ViewBag.errorSubGroup" disabled class="form-control" readonly></select>

    </div>

    <div class="form-group">
        <label asp-for="ErrorDetailGroupId"></label>
        <select asp-for="ErrorDetailGroupId" asp-items="ViewBag.errorDetailList" disabled class="form-control" readonly></select>

    </div>


    <button type="submit" class="btn btn-primary" onclick="return confirm('Silmek istediğinize emin misiniz?')">Delete</button>

</form>
