@model Core.DTOs.PatternDto

<button class="btn btn-outline-secondary" onclick="window.history.go(-1);">
    <i class="bi bi-arrow-left"></i> Geri
</button>

<form asp-action="Delete" method="post" asp-route-id=@Model.Id asp-controller="Pattern">


    <div class="form-group">
        <label asp-for="CustomerId"></label>
        <select asp-for="CustomerId" asp-items="ViewBag.customers" disabled class="form-control" readonly></select>

    </div>

    <div class="form-group">
        <label asp-for="ProjectId"></label>
        <select asp-for="ProjectId" asp-items="ViewBag.projects" disabled  class="form-control" readonly></select>

    </div>

    <div class="form-group">
        <label asp-for="MontageLetterId"></label>
        <select asp-for="MontageLetterId" asp-items="ViewBag.montageLetters" disabled class="form-control" readonly></select>

    </div>

    <div class="form-group">
        <label asp-for="PartId"></label>
        <select asp-for="PartId" asp-items="ViewBag.parts" disabled class="form-control" readonly></select>

    </div>

    <div class="form-group">
        <label asp-for="OperationId"></label>
        <select asp-for="OperationId" asp-items="ViewBag.operations" disabled class="form-control" readonly></select>

    </div>


</form>
