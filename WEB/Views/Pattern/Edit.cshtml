@model Core.DTOs.PatternDto

<h2>Edit </h2>

<button class="btn btn-outline-secondary" onclick="window.history.go(-1);">
    <i class="bi bi-arrow-left"></i> Geri
</button>

<form asp-action="Edit" method="post" asp-controller="Pattern">


    <div class="form-group">
        <label asp-for="CustomerId"></label>
        <select asp-for="CustomerId" asp-items="ViewBag.customers" class="form-control"></select>

    </div>

    <div class="form-group">
        <label asp-for="ProjectId"></label>
        <select asp-for="ProjectId" asp-items="ViewBag.projects" class="form-control"></select>

    </div>

    <div class="form-group">
        <label asp-for="MontageLetterId"></label>
        <select asp-for="MontageLetterId" asp-items="ViewBag.montageLetters" class="form-control"></select>

    </div>

    <div class="form-group">
        <label asp-for="PartId"></label>
        <select asp-for="PartId" asp-items="ViewBag.parts" class="form-control"></select>

    </div>

    <div class="form-group">
        <label asp-for="OperationId"></label>
        <select asp-for="OperationId" asp-items="ViewBag.operations" class="form-control"></select>

    </div>


    <button type="submit" class="btn btn-primary">Update</button>
</form>
